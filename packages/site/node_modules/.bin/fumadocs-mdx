#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/fumadocs-mdx@14.2.5_@types+react@19.2.8_fumadocs-core@16.4.7_@types+react@19.2.8_lucide_cccf830c703a9126f329292c66230c6d/node_modules/fumadocs-mdx/dist/node_modules:/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/fumadocs-mdx@14.2.5_@types+react@19.2.8_fumadocs-core@16.4.7_@types+react@19.2.8_lucide_cccf830c703a9126f329292c66230c6d/node_modules/fumadocs-mdx/node_modules:/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/fumadocs-mdx@14.2.5_@types+react@19.2.8_fumadocs-core@16.4.7_@types+react@19.2.8_lucide_cccf830c703a9126f329292c66230c6d/node_modules:/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/fumadocs-mdx@14.2.5_@types+react@19.2.8_fumadocs-core@16.4.7_@types+react@19.2.8_lucide_cccf830c703a9126f329292c66230c6d/node_modules/fumadocs-mdx/dist/node_modules:/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/fumadocs-mdx@14.2.5_@types+react@19.2.8_fumadocs-core@16.4.7_@types+react@19.2.8_lucide_cccf830c703a9126f329292c66230c6d/node_modules/fumadocs-mdx/node_modules:/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/fumadocs-mdx@14.2.5_@types+react@19.2.8_fumadocs-core@16.4.7_@types+react@19.2.8_lucide_cccf830c703a9126f329292c66230c6d/node_modules:/Users/steedos/Documents/GitHub/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fumadocs-mdx/dist/bin.js" "$@"
else
  exec node  "$basedir/../fumadocs-mdx/dist/bin.js" "$@"
fi
